{
  "name": "opencollective-rss",
  "version": "1.0.0",
  "description": "A lightweight TypeScript service to generate RSS feeds for updates from any Open Collective account, using GraphQL and Hono.",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "codegen": "graphql-codegen",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "keywords": [
    "opencollective",
    "rss",
    "graphql",
    "hono"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@hono/node-server": "^1.8.2",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "hono": "^4.0.5",
    "rss": "^1.2.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@graphql-codegen/typescript-operations": "^4.1.2",
    "@types/node": "^20.11.24",
    "@types/rss": "^0.0.32",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "supertest": "^7.1.1",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "vitest": "^3.0.0"
  }
}
